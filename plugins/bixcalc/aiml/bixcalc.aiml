<?xml version="1.0" encoding="UTF-8"?>
<aiml version="1.0">
  
<category>
<pattern>WHAT IS * PLUS *</pattern>
<template><system>bixcalc -a <star index="1"/>,<star index="2"/></system></template>
</category>

<category>
<pattern>ADD *</pattern>
<template><system>bixcalc -a "<star/>"</system></template>
</category>

<category>
